{
  "version": 2,
  "builds": [
    {
      "src": "embedding_server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"  // Increase the max size if you're dealing with larger payloads
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "embedding_server.py"
    }
  ]
}
